/* pages/me/me.wxss */

page {
  width: 100%;
  height: 100%;
  background: url("data:image/jpg;base64,") no-repeat;
  background-size: 100% 100%;
}

.userinfo {
  display: flex;
  flex-direction: column;
  align-items: center;
}

.userinfo-avatar {
  width: 60px;
  height: 60px;
  margin: 10px;
  border-radius: 50%;
}

.userinfo-nickname {
  color: #aaa;
}

.usermotto {
  margin-top: 200px;
}

.details {
  padding: 3%;
}

.list {
  width: 100%;
  height: 60px;
  line-height: 60px;
}

.img {
  width: 6%;
  height: 24px;
  float: left;
  margin-top: 18px;
}

.text {
  width: 94%;
  margin-left: 8%;
  box-sizing: border-box;
  border-bottom: 1px solid #eee;
}

.img1 {
  width: 6%;
  height: 24px;
  float: right;
  margin-top: 18px;
}

.callphone {
  width: 80%;
  height: 100px;
  margin: auto;
  background: #fff;
  display: none;
}

.calltext {
  text-align: center;
  height: 60px;
  line-height: 60px;
  font-weight: bold;
}

.yesno {
  height: 40px;
  line-height: 40px;
  text-align: center;
  border-top:1px solid #aaa;
}

.yesno view:nth-child(1) {
  width: 50%;
  color: black;
  float: left;
  border-right: 1px solid #aaa;
  box-sizing: border-box;
}

.yesno view:nth-child(2) {
  width: 50%;
  color: green;
  float: left;
}

.phone{
  position: fixed;
  left: 0;
  top:0;
  width:100%;
  height:100%;
  background:  rgba(0, 0, 0, .3);
}
.callphone {
  display: block;
  position: fixed;
  margin: auto;
  left: 0;
  right: 0;
  top: 0;
  bottom: 0;
}
.showphone{
  display: none;
}
